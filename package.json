  "dependencies": {
    "@aws-sdk/client-ses": "^3.321.1",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-engine-jsx": "^3.5.0",
    "mysql2": "^3.2.3",
    "sequelize": "^6.31.0"
  },
  "devDependencies": {
    "browser-sync": "^2.29.1",
